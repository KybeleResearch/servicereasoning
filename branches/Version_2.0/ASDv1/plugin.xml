<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="WSDL2ASDLaunchConfigurationDelegate"
            id="ASDv1.WSDL2ASDv1_lct"
            modes="run,debug"
            name="WSDL1.1 -&gt;ASDASD"
            public="true">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="WSDL2ASDLaunchConfigurationTabGroup"
            id="ASDv1.WSDL2ASDv1_lctg"
            type="ASDv1.WSDL2ASDv1_lct">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="WSDL2ASDLaunchShortcut"
            id="ASDv1.WSDL2ASDv1_ls"
            label="WSDL1.1-&gt;ASD"
            modes="run, debug">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <instanceof
                           value="org.eclipse.core.resources.IFile">
                     </instanceof>
                     <test
                           property="org.eclipse.debug.ui.matchesPattern"
                           value="*.xmi">
                     </test>
                  </iterate>
               </with></enablement>
            <contextLabel
                  label="WSDL1.1-&gt;ASD"
                  mode="run">
            </contextLabel></contextualLaunch>
         <configurationType
               id="ASDv1.WSDL2ASDv1_lct">
         </configurationType>
      </shortcut>
      <shortcut
            class="WSDL2ASDLaunchShortcut"
            id="ASDv1.WSDL2ASDv1_ls_edit"
            label="WSDL1.1-&gt;ASD Extension Editor"
            modes="run, debug">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <instanceof
                           value="org.eclipse.core.resources.IFile">
                     </instanceof>
                     <test
                           property="org.eclipse.debug.ui.matchesPattern"
                           value="*.wsdlv1">
                     </test>
                  </iterate>
               </with>
            </enablement>
            <contextLabel
                  label="WSDL1.1-&gt;ASD"
                  mode="run">
            </contextLabel>
         </contextualLaunch>
         <configurationType
               id="ASDv1.WSDL2ASDv1_lct">
         </configurationType>
      </shortcut>
      <shortcut
            class="WSDL2ASDLaunchShortcut"
            id="ASDv1.WSDL2ASDv1_ls_ecore"
            label="WSDL1.1-&gt;ASD Ecore"
            modes="run, debug">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <instanceof
                           value="org.eclipse.core.resources.IFile">
                     </instanceof>
                     <test
                           property="org.eclipse.debug.ui.matchesPattern"
                           value="*.ecore">
                     </test>
                  </iterate>
               </with>
            </enablement>
            <contextLabel
                  label="WSDL1.1-&gt;ASD"
                  mode="run">
            </contextLabel>
         </contextualLaunch>
         <configurationType
               id="ASDv1.WSDL2ASDv1_lct">
         </configurationType>
      </shortcut>
   </extension>

</plugin>
